{
    "name": "ispahbod/http-manager",
    "description": "http-manager for Laravel Framework",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "laravel",
        "framework",
        "php"
    ],
    "authors": [{
        "name": "Hamed Hadi",
        "email": "hamed.hadi1010@gmail.com",
        "homepage": "https://github.com/ispahbod",
        "role": "Developer"
    }],
    "homepage": "https://github.com/ispahbod/http-manager",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.8",
        "ispahbod/url-manager": "^1.0.0",
        "illuminate/support": ">=6",
        "symfony/dom-crawler": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "psr-4": {
            "Ispahbod\\HttpManager\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ispahbod\\HttpManager\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "beta"
}
